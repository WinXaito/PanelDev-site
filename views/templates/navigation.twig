                {% if fav.2 is not empty %}<li class="item-favori {{ tab.fav.7 }}"><a href="">Favori 7</a></li>{% endif %}
                {% if fav.2 is not empty %}<li class="item-favori {{ tab.fav.8 }}"><a href="">Favori 8</a></li>{% endif %}
                {% if fav.2 is not empty %}<li class="item-favori {{ tab.fav.9 }}"><a href="">Favori 9</a></li>{% endif %}
                {% if fav.2 is not empty %}<li class="item-favori {{ tab.fav.10 }}"><a href="">Favori 10</a></li>{% endif %}
            </ul>
        {% endif %}
    </aside>
{% endif %}